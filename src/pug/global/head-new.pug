mixin head(title)
	head
		meta(charset="utf-8")
		title= title
		meta(name="keywords" content="keywords")
		meta(name="description" content="description")
		//favicon
		//-mixin
		mixin favicon(rel, sizes, href, type)
			link(rel= rel, sizes= sizes, href= href, type= type)
		//viewport
		meta(name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no, maximum-scale=1.0")
		//- meta(name="viewport" content="width=device-width, initial-scale=1.0")
		link(rel="stylesheet" href="http://jscrollpane.kelvinluck.com/style/jquery.jscrollpane.css")
		//jPList Core
		link(href="https://cdnjs.cloudflare.com/ajax/libs/jplist/5.2.0/css/jplist.core.min.css" rel="stylesheet" type="text/css")
		link(rel="stylesheet" href="assets/css/app.min.css")
